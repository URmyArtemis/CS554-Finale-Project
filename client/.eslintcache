nents/App.js",[],"/Users/jiaqili/Desktop/554final/CS554-Finale-Project/client/src/components/Landing.js",[],"/Users/jiaqili/Desktop/554final/CS554-Finale-Project/client/src/components/PrivateRoute.js",[],"/Users/jiaqili/Desktop/554final/CS554-Finale-Project/client/src/components/Navigation.js",[],"/Users/jiaqili/Desktop/554final/CS554-Finale-Project/client/src/components/Home.js",[],"/Users/jiaqili/Desktop/554final/CS554-Finale-Project/client/src/components/SignUp.js",[],"/Users/jiaqili/Desktop/554final/CS554-Finale-Project/client/src/components/SignIn.js",[],"/Users/jiaqili/Desktop/554final/CS554-Finale-Project/client/src/components/Account.js",[],"/Users/jiaqili/Desktop/554final/CS554-Finale-Project/client/src/components/BusinessList.js",["83","84","85","86"],"import React, { useState } from 'react';\nimport { useQuery } from '@apollo/client';\nimport { Link } from 'react-router-dom';\nimport SearchBusinesses from './SearchBusinesses'\nimport queries from '../queries';\nimport { Card, CardActionArea, CardContent, CardMedia, Grid, Typography, makeStyles } from '@material-ui/core';\nimport '../App.css';\n\nconst useStyles = makeStyles({\n    card: {\n        maxWidth: 250,\n        height: 'auto',\n        marginLeft: 'auto',\n        marginRight: 'auto',\n        borderRadius: 5,\n        border: '1px solid #1e8678',\n        boxShadow: '0 19px 38px rgba(0,0,0,0.30), 0 15px 12px rgba(0,0,0,0.22);'\n    },\n    titleHead: {\n        borderBottom: '1px solid #1e8678',\n        fontWeight: 'bold'\n    },\n    grid: {\n        flexGrow: 1,\n        flexDirection: 'row'\n    },\n    media: {\n        height: '100%',\n        width: '100%'\n    },\n    button: {\n        color: '#1e8678',\n        fontWeight: 'bold',\n        fontSize: 12\n    }\n});\n\nconst BusinessList = (props) => {\n    const classes = useStyles();\n    const [term, setTerm] = useState(undefined);\n    const [location, setLocation] = useState('Hoboken');\n    const { data, loading, error } = useQuery(queries.GET_YELPBUSINESSES, {\n        variables: {\n            term: term,\n            location: location\n        }\n    });\n    let card = null;\n\n    const searchTerm = (term) => {\n        setTerm(term);\n    }\n\n    const searchLocation = (location) => {\n        setLocation(location);\n    }\n\n    const buildCard = (business) => {\n        return (\n            <Grid item xs={12} sm={6} md={4} lg={3} xl={2} key={business.id}>\n                <Card className={classes.card} variant='outlined'>\n                    <CardActionArea>\n                        <Link to={`/businesses/${business.id}`}>\n                            <CardMedia\n                                className={classes.media}\n                                component='img'\n                                image='../img/${business.alias}.jpg'\n                                // image={business.image_url}\n                                title='business image'\n                            />\n\n                            <CardContent>\n                                <Typography className={classes.titleHead} gutterBottom variant='h6' component='h3'>\n                                    {business.name}\n                                </Typography>\n                                <Typography variant='body2' color='textSecondary' component='p'>\n                                    rating: {business.rating}\n                                </Typography>\n                                <Typography variant='body2' color='textSecondary' component='p'>\n                                    price: {business.price}\n                                </Typography>\n                            </CardContent>\n                        </Link>\n                    </CardActionArea>\n                </Card>\n            </Grid>\n        )\n    }\n\n    if (loading) {\n        return <div>Loading...</div>\n    } else if (error) {\n        return <div>{error.message}</div>\n    }\n\n    const { yelpBusinesses } = data;\n    card = yelpBusinesses.map((business) => {\n        return buildCard(business);\n    });\n\n    return (\n        <div>\n            {/* <SearchBusinesses searchTerm={searchTerm} searchLocation={searchLocation} /> */}\n            <br />\n            <br />\n            <Grid container className={classes.grid} spacing={5}>\n                {card}\n            </Grid>\n        </div>\n    )\n\n};\n\nexport default BusinessList;","/Users/jiaqili/Desktop/554final/CS554-Finale-Project/client/src/firebase/Auth.js",[],"/Users/jiaqili/Desktop/554final/CS554-Finale-Project/client/src/components/SignOut.js",[],"/Users/jiaqili/Desktop/554final/CS554-Finale-Project/client/src/components/SocialSignIn.js",[],"/Users/jiaqili/Desktop/554final/CS554-Finale-Project/client/src/firebase/FirebaseFunctions.js",[],"/Users/jiaqili/Desktop/554final/CS554-Finale-Project/client/src/components/ChangePassword.js",[],"/Users/jiaqili/Desktop/554final/CS554-Finale-Project/client/src/firebase/Firebase.js",[],"/Users/jiaqili/Desktop/554final/CS554-Finale-Project/client/src/components/SearchBusinesses.js",[],"/Users/jiaqili/Desktop/554final/CS554-Finale-Project/client/src/queries.js",["87"],"import { gql } from \"@apollo/client\";\n\nconst GET_YELPBUSINESSES = gql`\n    query ($term: String, $location: String!) {\n        yelpBusinesses(term: $term, location: $location) {\n            id\n            alias\n            name\n            image_url\n            rating\n            location\n            display_phone\n            price\n        }        \n    }\n`;\n\nconst GET_BUSINESSREVIEWS = gql`\n    query ($alias: String!) {\n        businessReviews(alias: $alias) {\n            id\n            text\n            rating\n            time_created\n            username\n        }\n    }\n`;\n\nconst GET_BINNEDBUSINESSES = gql`\n    query ($uid: ID!) {\n        binnedBusinesses(uid: $uid) {\n            id\n            alias\n            name\n            image_url\n            rating\n            location\n            display_phone\n            price\n        }\n    }\n`;\n\nconst GET_POSTEDREVIEWS = gql`\n    query ($uid: ID!) {\n        postedReviews(uid: $uid) {\n            id\n            text\n            rating\n            time_created\n            username\n        }\n    }\n`;\n\nconst UPLOAD_REVIEW = gql`\n    mutation ($businessAlias: String!, $text: String!, $rating: Int!, $username: String!) {\n        uploadReview(businessAlias: $businessAlias, text: $text, rating: $rating, username: $username) {\n            id\n            text\n            rating\n            time_created\n            username\n        }\n    }\n`;\n\nconst UPDATE_REVIEW = gql`\n    mutation ($businessAlias: String!, $id: ID!, $text: String, $rating: Int) {\n        updateReview(businessAlias: $businessAlias, id: $id, text: $text, rating: $rating) {\n            id\n            text\n            rating\n            time_created\n            username\n        }\n    }\n`;\n\nconst DELETE_REVIEW = gql`\n    mutation ($businessAlias: String!, $id: ID!) {\n        deleteReview(businessAlias: $businessAlias, id: $id) {\n            id\n            text\n            rating\n            time_created\n            username\n        }\n    }\n`;\n\nconst UPDATE_BUSINESS = gql`\n    mutation ($id: ID!, $binned: Boolean!) {\n        updateBusiness(id: $id, binned: $binned) {\n            id\n            alias\n            name\n            image_url\n            rating\n            location\n            display_phone\n            price\n        }\n    }\n`;\n\nexport default {\n    GET_YELPBUSINESSES,\n    GET_BUSINESSREVIEWS,\n    GET_BINNEDBUSINESSES,\n    GET_POSTEDREVIEWS,\n    UPLOAD_REVIEW,\n    UPDATE_REVIEW,\n    DELETE_REVIEW,\n    UPDATE_BUSINESS\n}",{"ruleId":"88","replacedBy":"89"},{"ruleId":"90","replacedBy":"91"},{"ruleId":"92","severity":1,"message":"93","line":4,"column":8,"nodeType":"94","messageId":"95","endLine":4,"endColumn":24},{"ruleId":"92","se